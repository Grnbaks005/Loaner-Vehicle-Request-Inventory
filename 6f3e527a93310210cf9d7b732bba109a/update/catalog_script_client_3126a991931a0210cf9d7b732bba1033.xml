        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below
   var neededDate = g_form.getValue('date_needed');
   var returnDate = g_form.getValue('date_to_return_the_vehicle'); 
	
   // If return date is before date needed, trigger error message
	if (returnDate < neededDate) {
		g_form.addErrorMessage('Return date cannot be before needed date');
		g_form.clearValue('date_to_return_the_vehicle'); 
		return false; // Prevent form submission
		
	}
	return true; // Allow form submission
}]]></script>
